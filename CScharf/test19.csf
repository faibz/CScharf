public class Rectangle {
	public Rectangle(int len, int wid) {
		length = len;
		width = wid;
	}
	
	int length = 12;
	int width = 8;
	instance innerRect;
	
	public int SetLength(int newLen) {
		write("Setting length to: " + newLen);
		length = newLen;
		
		return -1;
	}
	
	public int GetArea() {
		return length * width;
	}
	
	public int DeepFunctionCall() {
		write(innerRect.GetArea());
		
		return -1;
	}
}

